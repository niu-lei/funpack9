    <Block>
        <Name>Buffer Out</Name>
        <DisplayName>Buffer##Out</DisplayName>
        <Version>1.0</Version>
        <LibName>Buffers</LibName>
        <BlockNumber>1</BlockNumber>
        <Description></Description>
        <X>316</X>
        <Y>-56</Y>
        <Width>60</Width>
        <Height>40</Height>
        <Inputs>
            <Input>
                <Type>FLOAT</Type>
                <Size>%BufferSize%</Size>
                <Name>in1</Name>
                <Description>Input buffer.</Description>
            </Input>
            <Input>
                <Type>INT</Type>
                <Size>1</Size>
                <Name>in2</Name>
                <Description>Index which item should be extracted.</Description>
            </Input>
        </Inputs>
        <Outputs>
            <Output>
                <Type>FLOAT</Type>
                <Size>1</Size>
                <Name>out1</Name>
                <Description>Selected item from the input buffer.</Description>
            </Output>
        </Outputs>
        <Properties>
            <Property>
                <Type>INT</Type>
                <Name>BufferSize</Name>
                <Value>1</Value>
                <Description>Size of the input buffer.</Description>
            </Property>
        </Properties>
        <Memory/>
        <Command>
            <Line>if (%in2%[0] &lt; %BufferSize%) %out1%[0] = %in1%[%in2%[0]];</Line>
        </Command>
        <InitCode/>
        <Function/>
    </Block>
    <Block>
        <Name>[Sensor Hub]</Name>
        <DisplayName>[Sensor Hub]</DisplayName>
        <Version>3.0</Version>
        <LibName>Sensor Hub</LibName>
        <BlockNumber>1</BlockNumber>
        <Description>Sensor Hub provides links to sensor and algorithms in order to retrieve data from them.</Description>
        <X>-144</X>
        <Y>21</Y>
        <Width>100</Width>
        <Height>40</Height>
        <Inputs/>
        <Outputs>
            <Output>
                <Type>VOID</Type>
                <Size>1</Size>
                <Name>out</Name>
                <Description>Sensor Hub handler.</Description>
                <Node>Sensor_Hub_1_out</Node>
            </Output>
        </Outputs>
        <Properties>
            <Property>
                <Type>INT</Type>
                <Name>Data Rate Control</Name>
                <Value>0</Value>
                <Enum>Timer=0,Accelerometer=1,Gyroscope=2,Offline Data=255</Enum>
                <Description>Defines the data rate source.</Description>
            </Property>
            <Property>
                <Type>INT</Type>
                <Name>Data Rate</Name>
                <Value>100</Value>
                <Min>1</Min>
                <Max>400</Max>
                <Description>Defines the sensor's and algorithm's data rate in Hz. If accelerometer or gyroscope is selected as data rate source, the closest higher available output data rate (ODR) is selected.</Description>
            </Property>
            <Property>
                <Type>INT</Type>
                <Name>Accelerometer Full Scale</Name>
                <Value>1</Value>
                <Enum>2 g=0,4 g=1,8 g=2,16 g=3</Enum>
                <Description>Defines the accelerometer full scale.</Description>
            </Property>
            <Property>
                <Type>INT</Type>
                <Name>Gyroscope Full Scale</Name>
                <Value>1</Value>
                <Enum>245 dps=0,500 dps=1,1000 dps=2,2000 dps=3</Enum>
                <Description>Defines the gyroscope full scale.</Description>
            </Property>
        </Properties>
        <Memory/>
        <Command>
            <Line>Sensor_Hub_Handler(&amp;%out%);</Line>
        </Command>
        <InitCode>
            <Line>Sensor_Hub_Init(%Data Rate Control%, %Data Rate%, %Accelerometer Full Scale%, %Gyroscope Full Scale%);</Line>
        </InitCode>
        <Function/>
    </Block>
    <Block>
        <Name>Angular Rate [dps]</Name>
        <DisplayName>Angular Rate##[dps]</DisplayName>
        <Version>2.1</Version>
        <LibName>Sensor Hub</LibName>
        <BlockNumber>1</BlockNumber>
        <Description>Retrieve the gyroscope data.</Description>
        <Dependency>Gyroscope</Dependency>
        <X>104</X>
        <Y>-28</Y>
        <Width>100</Width>
        <Height>40</Height>
        <Inputs>
            <Input>
                <Type>VOID</Type>
                <Size>1</Size>
                <Name>SensorHubHandle</Name>
                <Description>Connect to Sensor Hub output.</Description>
                <Node>Sensor_Hub_1_out</Node>
            </Input>
        </Inputs>
        <Outputs>
            <Output>
                <Type>FLOAT</Type>
                <Size>3</Size>
                <Name>data</Name>
                <Description>Gyroscope data (angular rate) in dps.</Description>
            </Output>
        </Outputs>
        <Properties/>
        <Memory/>
        <Command>
            <Line>Gyro_Sensor_GetData(%SensorHubHandle%, %data%);</Line>
        </Command>
        <InitCode>
            <Line>Gyro_Init();</Line>
        </InitCode>
        <Function/>
    </Block>
</Project>
